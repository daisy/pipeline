    <x:scenario label="Element :: a">
        <x:scenario label="element and allowed attributes">
            <x:context>
                <a id="foo" foo="bar" epub:type="foo" href="#foo" target="_self" download=""
                    rel="alternate" hreflang="en" type="text/html"/>
            </x:context>
            <x:expect label="are translated as-is">
                <a id="foo" epub:type="foo" href="#foo" target="_self" download="" rel="alternate"
                    hreflang="en" type="text/html"/>
            </x:expect>
        </x:scenario>
        <x:scenario label="name attribute">
            <x:scenario label="when no id is already present">
                <x:context>
                    <a name="foo"/>
                </x:context>
                <x:expect label="is translated as id">
                    <a id="foo"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="with an empty value">
                <x:context>
                    <a name=""/>
                </x:context>
                <x:expect label="is discarded">
                    <a/>
                </x:expect>
            </x:scenario>
            <x:scenario label="when an id is already present">
                <x:context>
                    <a name="foo" id="bar"/>
                </x:context>
                <x:expect label="is discarded" test="h:a">
                    <a id="bar"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="with a value of an existing ID">
                <x:context>
                    <a name="foo"/>
                    <span id="foo"/>
                </x:context>
                <x:expect label="is discarded" test="h:a">
                    <a/>
                </x:expect>
            </x:scenario>
        </x:scenario>
        <x:scenario label="with a coords or attribute and an area content model">
            <x:context>
                <map>
                    <a id="foo" foo="bar" epub:type="foo" href="#foo" target="_self" download=""
                        rel="alternate" hreflang="en" type="text/html" coords="1 2" shape="circle"/>
                </map>
            </x:context>
            <x:expect label="is converted to area">
                <map>
                    <area id="foo" epub:type="foo" href="#foo" target="_self" download=""
                        rel="alternate" hreflang="en" type="text/html" coords="1 2" shape="circle"/>
                </map>
            </x:expect>
        </x:scenario>
        <x:scenario label="with a shape or coords attribute and non-empty">
            <x:context>
                <a id="foo" foo="bar" epub:type="foo" href="#foo" target="_self" download=""
                    rel="alternate" hreflang="en" type="text/html" coords="1 2" shape="circle"
                    >test</a>
            </x:context>
            <x:expect label="is kept as an a, and attributes are removed">
                <a id="foo" epub:type="foo" href="#foo" target="_self" download="" rel="alternate"
                    hreflang="en" type="text/html">test</a>
            </x:expect>
        </x:scenario>
    </x:scenario>

