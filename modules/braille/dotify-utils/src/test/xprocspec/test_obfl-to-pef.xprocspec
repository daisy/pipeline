<?xml version="1.0" encoding="UTF-8"?>
<x:description xmlns:x="http://www.daisy.org/ns/xprocspec"
               xmlns:p="http://www.w3.org/ns/xproc"
               xmlns:px="http://www.daisy.org/ns/pipeline/xproc"
               xmlns:pxi="http://www.daisy.org/ns/pipeline/xproc/internal"
               xmlns:dp2="http://www.daisy.org/ns/pipeline/"
               xmlns:obfl="http://www.daisy.org/ns/2011/obfl"
               xmlns:pef="http://www.daisy.org/ns/2008/pef">
  
  <x:script>
    <p:declare-step type="pxi:test" version="1.0">
      <p:input port="source"/>
      <p:output port="result"/>
      <p:option name="text-transform" select="'(input:braille)'"/>
      <p:option name="braille-charset" select="''"/>
      <p:import href="../../main/resources/xml/obfl-normalize-space.xpl"/>
      <p:import href="http://www.daisy.org/pipeline/modules/braille/dotify-utils/library.xpl"/>
      <p:choose>
        <p:when test="not(exists(/obfl:obfl/meta))">
          <p:insert match="/obfl:obfl" position="first-child">
            <p:input port="insertion">
              <p:inline>
                <obfl:meta/>
              </p:inline>
            </p:input>
          </p:insert>
        </p:when>
        <p:otherwise>
          <p:identity/>
        </p:otherwise>
      </p:choose>
      <p:choose>
        <p:when test="$braille-charset!=''">
          <p:insert match="/obfl:obfl/obfl:meta" position="last-child">
            <p:input port="insertion">
              <p:inline>
                <dp2:braille-charset>{}</dp2:braille-charset>
              </p:inline>
            </p:input>
          </p:insert>
          <p:string-replace match="/obfl:obfl/obfl:meta/dp2:braille-charset/text()">
            <p:with-option name="replace" select="concat('&quot;',$braille-charset,'&quot;')"/>
          </p:string-replace>
        </p:when>
        <p:otherwise>
          <p:identity/>
        </p:otherwise>
      </p:choose>
      <p:group>
        <p:insert match="/obfl:obfl/obfl:meta" position="last-child">
          <p:input port="insertion">
            <p:inline>
              <dp2:default-mode>{}</dp2:default-mode>
            </p:inline>
          </p:input>
        </p:insert>
        <p:string-replace match="/obfl:obfl/obfl:meta/dp2:default-mode/text()">
          <p:with-option name="replace" select='concat("&apos;",$text-transform,"&apos;")'/>
        </p:string-replace>
      </p:group>
      <pxi:obfl-normalize-space/>
      <px:obfl-to-pef/>
      <p:delete match="/pef:pef/pef:head/pef:meta/dp2:ascii-braille-charset"/>
    </p:declare-step>
  </x:script>
  
