              <template use-when="(= (% $page 2) 1)">
                <header>
                  <field>
                    <compound-marker-reference>
                      <marker-reference marker="foo/prev" direction="forward" scope="spread-content" start-offset="-1"/>
                      <marker-reference marker="foo" direction="backward" scope="sequence"/>
                    </compound-marker-reference>
                  </field>
                  <field/>
                </header>
                <footer>
                  <field>
