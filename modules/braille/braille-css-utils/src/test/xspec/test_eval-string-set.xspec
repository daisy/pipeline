<?xml version="1.0" encoding="utf-8"?>
<x:description xmlns:x="http://www.jenitennison.com/xslt/xspec"
               xmlns:css="http://www.daisy.org/ns/pipeline/braille-css"
               stylesheet="../../main/resources/xml/eval-string-set.xsl">
  
  <x:scenario label="test_01">
    <x:context>
      <doc>
        <body>
          <pagenum num="1" css:string-set="print-page attr(num)"/>
        </body>
      </doc>
    </x:context>
    <x:expect label="result">
      <doc>
        <body>
          <pagenum num="1" css:string-set="print-page &quot;1&quot;"/>
        </body>
      </doc>
    </x:expect>
  </x:scenario>
  
  <x:scenario label="test_02">
    <x:context>
      <doc>
        <body>
          <h style="display: block" css:string-set="running-header content()">
            foo
          </h>
        </body>
      </doc>
    </x:context>
    <x:expect label="result">
      <doc>
        <body>
          <h style="display: block" css:string-set="running-header &quot;\A             foo\A           &quot;">
            foo
          </h>
        </body>
      </doc>
    </x:expect>
  </x:scenario>
  
</x:description>
