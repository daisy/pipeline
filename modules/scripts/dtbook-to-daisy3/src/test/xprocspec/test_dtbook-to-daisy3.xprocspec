						<book>
							<frontmatter>
								<doctitle id="doctitle">title</doctitle>
							</frontmatter>
							<bodymatter>
								<level1>
									<h1 id="ch1">chapter</h1>
									<p id="p1">
										<m:math id="m1">
											<m:mrow>
												<m:mo>a &lt; b</m:mo>
											</m:mrow>
										</m:math>
									</p>
								</level1>
							</bodymatter>
